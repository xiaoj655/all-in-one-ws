#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/Brook/repo/all-in-one-ws/web/web12/node_modules/.pnpm/r2.cnpmjs.org+atob@2.1.2/node_modules/atob/bin/node_modules:/home/Brook/repo/all-in-one-ws/web/web12/node_modules/.pnpm/r2.cnpmjs.org+atob@2.1.2/node_modules/atob/node_modules:/home/Brook/repo/all-in-one-ws/web/web12/node_modules/.pnpm/r2.cnpmjs.org+atob@2.1.2/node_modules:/home/Brook/repo/all-in-one-ws/web/web12/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/Brook/repo/all-in-one-ws/web/web12/node_modules/.pnpm/r2.cnpmjs.org+atob@2.1.2/node_modules/atob/bin/node_modules:/home/Brook/repo/all-in-one-ws/web/web12/node_modules/.pnpm/r2.cnpmjs.org+atob@2.1.2/node_modules/atob/node_modules:/home/Brook/repo/all-in-one-ws/web/web12/node_modules/.pnpm/r2.cnpmjs.org+atob@2.1.2/node_modules:/home/Brook/repo/all-in-one-ws/web/web12/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../r2.cnpmjs.org+atob@2.1.2/node_modules/atob/bin/atob.js" "$@"
else
  exec node  "$basedir/../../../../../r2.cnpmjs.org+atob@2.1.2/node_modules/atob/bin/atob.js" "$@"
fi
